{
  "name": "ep_profile_modal",
  "version": "1.0.55",
  "description": "profile modal for Etherpad",
  "main": "profile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "gulp js minify-css minify-image",
    "build:prod": "gulp build --production",
    "lint:fix": "eslint --fix ."
  },
  "eslintConfig": {
    "root": true,
    "extends": "etherpad/plugin",
    "ignorePatterns": [
      "/static/dist/**/*.js"
    ],
    "globals": {
      "$": true,
      "documentReady": true,
      "contributors": true,
      "profileForm": true,
      "usersProfileSection": true,
      "postAceInit": true,
      "aceInitialized": true,
      "handleClientMessage": true,
      "helper": true,
      "shared": true,
      "syncData": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samirsayyad/ep_profile_modal.git"
  },
  "keywords": [
    "Etherpad",
    "Profile"
  ],
  "author": {
    "name": "Samir Sayyad",
    "email": "samir.saiad@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samirsayyad/ep_profile_modal/issues"
  },
  "homepage": "https://github.com/samirsayyad/ep_profile_modal#readme",
  "dependencies": {
    "aws-sdk": "^2.1075.0",
    "busboy": "^1.4.0",
    "gravatar": "^1.8.2",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.7.2",
    "sharp": "^0.30.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-etherpad": "^1.0.23",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-git": "^2.10.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject-string": "^1.1.2",
    "gulp-mode": "^1.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify-es": "^2.0.0"
  }
}
