{
  "name": "ep_profile_modal",
  "version": "1.0.64",
  "description": "profile modal for Etherpad",
  "main": "profile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "gulp js minify-css minify-image",
    "build:prod": "gulp build --production",
    "lint:fix": "eslint --fix .",
    "lint:dry": "eslint --fix-dry-run ."
  },
  "eslintConfig": {
    "root": true,
    "extends": "etherpad/plugin",
    "ignorePatterns": [
      "/static/dist/**/*.js"
    ],
    "globals": {
      "$": true,
      "documentReady": true,
      "contributors": true,
      "profileForm": true,
      "usersProfileSection": true,
      "postAceInit": true,
      "aceInitialized": true,
      "handleClientMessage": true,
      "helper": true,
      "shared": true,
      "syncData": true,
      "__LOGIN": true,
      "__LOGOUT": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samirsayyad/ep_profile_modal.git"
  },
  "keywords": [
    "Etherpad",
    "Profile"
  ],
  "author": {
    "name": "Samir Sayyad",
    "email": "samir.saiad@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samirsayyad/ep_profile_modal/issues"
  },
  "homepage": "https://github.com/samirsayyad/ep_profile_modal#readme",
  "dependencies": {
    "async": "^3.2.3",
    "aws-sdk": "^2.1098.0",
    "buffer-image-size": "^0.6.4",
    "busboy": "^1.5.0",
    "gravatar": "^1.8.2",
    "image-size": "^1.0.1 ",
    "node-fetch": "^2",
    "nodemailer": "^6.7.3",
    "path": "^0.12.7",
    "resize-img": "^2.0.0",
    "sharp": "^0.30.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-git": "^2.10.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject-string": "^1.1.2",
    "gulp-mode": "^1.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify-es": "^2.0.0",
    "prettier": "2.6.2"
  }
}
